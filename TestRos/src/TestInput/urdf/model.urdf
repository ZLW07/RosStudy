<robot name="mymodel">
    <link name="base_link">
        <visual>
            <geometry>
                <mesh filename="package://TestInput/urdf/model/TX2-40-HB-base_link.STL"/>
                <!--                <box size="0.5 0.2 0.1" />-->
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <link name="link_1">
        <visual>
            <geometry>
                <mesh filename="package://TestInput/urdf/model/TX2-40-HB-SHOULDER.STL"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="baseTolink1" type="continuous">
        <parent link="base_link"/>
        <child link="link_1"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_2">
