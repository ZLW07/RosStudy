        <child link="link_4"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_5">
        <visual>
            <geometry>
                <mesh filename="package://TestInput/urdf/model/TX2-40-HB-WRIST.STL"/>
