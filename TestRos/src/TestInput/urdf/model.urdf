                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="link1Tolink2" type="continuous">
        <parent link="link_1"/>
        <child link="link_2"/>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_3">
        <visual>
            <geometry>
                <mesh filename="package://TestInput/urdf/model/TX2-40-HB-ELBOW.STL"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
