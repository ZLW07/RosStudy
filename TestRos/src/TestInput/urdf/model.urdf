    </link>
    <joint name="link_2Tolink_3" type="continuous">
        <parent link="link_2"/>
        <child link="link_3"/>
        <origin xyz="0 0.0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_4">
        <visual>
            <geometry>
                <mesh filename="package://TestInput/urdf/model/TX2-40-HB-FOREARM.STL"/>
            </geometry>
