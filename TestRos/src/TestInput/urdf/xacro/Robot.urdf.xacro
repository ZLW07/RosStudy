<!--<robot name="my_robot" xmlns:xacro="http://wiki.ros.org/xacro">-->
<robot name="mycar" xmlns:xacro="http://wiki.ros.org/xacro">

    <xacro:property name="PI" value="3.1415926"/>

    <xacro:property name="TrolleyPath" value="package://TestInputModel/urdf/Trolley.stl"/>
    <xacro:property name="Link1Path" value="package://TestInputModel/urdf/Link1.stl"/>
    <xacro:property name="Link2Path" value="package://TestInputModel/urdf/Link2.stl"/>
    <xacro:property name="Link3Path" value="package://TestInputModel/urdf/Link3.stl"/>
    <xacro:property name="Link4Path" value="package://TestInputModel/urdf/Link4.stl"/>
    <xacro:property name="Link5Path" value="package://TestInputModel/urdf/Link5.stl"/>

    <link name="base_link">
        <visual>
            <geometry>
                <mesh filename="${TrolleyPath}"/>
                <!--                <box size="0.5 0.2 0.1" />-->
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <link name="link_1">
        <visual>
            <geometry>
                <mesh filename="${Link1Path}"/>
                <!--                <box size="0.5 0.2 0.1" />-->
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1" />
            </material>
        </visual>
    </link>
    <joint name="baseTolink1" type="continuous">
        <parent link="base_link"/>
        <child link="link_1"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_2">
        <visual>
            <geometry>
                <mesh filename="${Link2Path}"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="link1Tolink2" type="continuous">
        <parent link="link_1"/>
        <child link="link_2"/>
        <origin xyz="0.0 0.0 0.0" rpy="-1.571 -1.571 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_3">
        <visual>
            <geometry>
                <mesh filename="${Link3Path}"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="link_2Tolink_3" type="continuous">
        <parent link="link_2"/>
        <child link="link_3"/>
        <origin xyz="0.4 0.0 0.02" rpy="0 0 1.571"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_4">
        <visual>
            <geometry>
                <mesh filename="${Link4Path}"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="link_3Tolink_4" type="continuous">
        <parent link="link_3"/>
        <child link="link_4"/>
        <origin xyz="0 -0.45 0" rpy="1.571 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link_5">
        <visual>
            <geometry>
                <mesh filename="${Link5Path}"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="link_4Tolink_5" type="continuous">
        <parent link="link_4"/>
        <child link="link_5"/>
        <origin xyz="0 0 0" rpy="-1.571 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>
</robot>
